# Development Environment Overrides
# These settings override base .env for local development

# Logging
AGENT_ENGINE_LOG_LEVEL=DEBUG
LOG_LEVEL=DEBUG
LOG_FORMAT=text

# Features
AGENT_ENGINE_ENABLE_TRACING=true
ENABLE_TRACING=true
ENABLE_METRICS=false

# ADK Configuration
ADK_SESSION_SERVICE=memory
ADK_TIMEOUT=60

# Performance (relaxed for development)
AGENT_ENGINE_MAX_CONCURRENT=5
AGENT_ENGINE_REQUEST_TIMEOUT=120
RATE_LIMIT_PER_MINUTE=120

# Security (relaxed for development)
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080", "*"]

# Database (development optimizations)
DATABASE_NAME=tahoe
DATABASE_MIN_CONNECTIONS=1
DATABASE_MAX_CONNECTIONS=5
DATABASE_CONNECTION_TIMEOUT=10

# Service URLs (development)
AUTH_SERVICE_URL=http://localhost:8002
BILLING_SERVICE_URL=http://localhost:8003

# Redis namespace (development)
AGENT_ENGINE_REDIS_NAMESPACE=agent:dev: